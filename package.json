{
  "name": "korttilaarnio",
  "version": "1.0.0",
  "description": "Scryfall integration for nodecg",
  "scripts": {
    "start": "parcel watch src/korttilaarnio.html --dist-dir graphics --public-url ./",
    "build": "parcel build src/korttilaarnio.html --dist-dir graphics --public-url ./",
    "lint": "eslint src/",
    "lint-fix": "eslint --fix src/",
    "start-nodecg": "node ../../index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aivoapina/credits-64.git"
  },
  "keywords": [
    "nodecg",
    "bundle",
    "credits",
    "streaming"
  ],
  "nodecg": {
    "compatibleRange": "^1.1.1",
    "dashboardPanels": [
      {
        "name": "Korttilaario",
        "title": "Korttilaario",
        "width": 3,
        "file": "korttilaarnio.html",
        "headerColor": "#525F78"
      }
    ],
    "graphics": [
      {
        "file": "korttilaarnio.html",
        "width": 1920,
        "height": 1080
      }
    ]
  },
  "author": {
    "name": "Aivoapina",
    "email": "anssi.varjonen@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Aivoapina/credits-64/issues"
  },
  "homepage": "https://github.com/Aivoapina/credits-64#readme",
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "eslint": "8.22.0",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "parcel": "2.7.0"
  }
}
